\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      form: {\r\n        username: 'admin',\r\n        password: '123123'\r\n      },\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入用户名', trigger: 'blur' }\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    submitForm (formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          localStorage.setItem('ms_username', this.form.username);\r\n          this.$router.push('/');\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.login-wrap {\r\n  position: relative;\r\n  background-image: url(../../assets/images/logo_bg.png);\r\n  width: 520px;\r\n  height: 302px;\r\n  padding: 110px 160px 0px 160px;\r\n  margin: 60px auto 0px auto;\r\n  overflow: hidden;\r\n  .ms-content {\r\n    padding: 30px 30px;\r\n  }\r\n  .login-btn {\r\n    text-align: center;\r\n  }\r\n  .login-btn button {\r\n    width: 220px;\r\n    height: 36px;\r\n    margin-bottom: 10px;\r\n    position: relative;\r\n    left: 25px;\r\n  }\r\n  img {\r\n    vertical-align: middle;\r\n    position: relative;\r\n    top: 70px;\r\n    left: 20px;\r\n  }\r\n}\r\n</style>\r\n"]}]}